{
  "$schema": "https://turborepo.org/schema.json",
  "extends": ["//"],
  "tasks": {
    "dev": {
      "persistent": true,
      "interactive": true
    },
    "build:ios": {
      "dependsOn": ["^build"],
      "outputs": [".cache/tsbuildinfo.json", "ios", ".expo", "build"],
      "env": ["APP_VARIANT"]
    },
    "build:android": {
      "dependsOn": ["^build"],
      "outputs": [".cache/tsbuildinfo.json", "android", ".expo", "build"],
      "env": ["APP_VARIANT"]
    },
    "build:update": {
      "dependsOn": ["^build"],
      "outputs": [".cache/tsbuildinfo.json", ".expo"],
      "env": ["APP_VARIANT"]
    }
  }
}
