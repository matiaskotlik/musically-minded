{
  "name": "@repo/mobile",
  "version": "1.0.0",
  "private": true,
  "main": "expo-router/entry",
  "scripts": {
    "android": "expo run:android",
    "build:android": "eas build --local ${CI+--non-interactive} --platform android --output build/app.apk",
    "build:check": "tsc --noEmit",
    "build:ios": "eas build --local ${CI+--non-interactive} --platform ios --output build/app.ipa",
    "build:update": "eas update --auto ${CI+--non-interactive}",
    "clean": "git clean -xdf .cache .expo .turbo/cache dist build android ios node_modules",
    "dev": "expo start",
    "dev:android": "expo start --android",
    "dev:ios": "expo start --ios",
    "format": "prettier --write --ignore-path ../../.gitignore --cache --cache-location .cache/.prettiercache",
    "format:check": "prettier --check --ignore-path ../../.gitignore --cache --cache-location .cache/.prettiercache",
    "ios": "expo run:ios",
    "lint": "expo lint --fix -- --cache-location .cache/.eslintcache",
    "lint:check": "expo lint -- --cache-location .cache/.eslintcache",
    "start": "expo start"
  },
  "prettier": "@repo/prettier-config",
  "dependencies": {
    "@repo/supabase": "workspace:*",
    "@repo/tsconfig": "workspace:*",
    "expo": "54.0.7",
    "expo-asset": "12.0.8",
    "expo-dev-client": "~6.0.12",
    "expo-router": "~6.0.4",
    "expo-splash-screen": "31.0.10",
    "expo-status-bar": "3.0.8",
    "expo-symbols": "1.0.7",
    "expo-system-ui": "6.0.7",
    "expo-updates": "29.0.10",
    "react": "catalog:react19",
    "react-dom": "catalog:react19",
    "react-native": "0.81.4"
  },
  "devDependencies": {
    "@repo/prettier-config": "workspace:*",
    "@repo/tsconfig": "workspace:*",
    "@types/react": "catalog:react19",
    "eslint": "catalog:",
    "prettier": "catalog:",
    "typescript": "catalog:"
  },
  "engines": {
    "node": "^22.16.0"
  }
}
